<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eauslander94.Courtlife" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hoophead</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="loadUrlTimeoutValue" value="7000000" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="http://192.168.1.123:8100" />
    <allow-navigation href="http://192.168.1.109:8100" />
    <allow-navigation href="http://192.168.0.12:8100" />
    <allow-navigation href="http://192.168.0.7:8100" />
    <allow-navigation href="http://192.168.0.4:8100" />
    <engine name="ios" spec="^4.4.0" />
    <engine name="android" spec="~6.3.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.4" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.1" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-plugin-safariviewcontroller" spec="^1.5.3" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-photo-library" spec="^2.1.0">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="To choose photos" />
    </plugin>
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.16" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="^4.3.0">
        <variable name="URL_SCHEME" value="com.eauslander94.courtlife" />
        <variable name="ANDROID_SCHEME" value="com.eauslander94.courtlife" />
        <variable name="ANDROID_HOST" value="eauslander94-dev.auth0.com" />
        <variable name="ANDROID_PATHPREFIX" value="/cordova/com.eauslander94.courtlife/callback" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="^4.0.2" />
    <plugin name="co.realtime.plugins.CordovaPush" spec="~0.2.2" />
    <plugin name="cordova-plugin-screen-orientation" spec="^2.0.2" />
</widget>
